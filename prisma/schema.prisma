datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Post {
  id        Int       @id @default(autoincrement())
  username  String
  content   String
  image     String
  createdAt DateTime  @default(now())
}
